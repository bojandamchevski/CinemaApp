@model List<UserListViewModel>

<h3>Users:</h3>
<br />

<ol>
    @foreach (var user in Model)
    {
        <li>@user.UserFullName</li>
        <ul>
            @foreach (var movie in @user.UserMovies)
            {
                <li>@movie</li>
            }
        </ul>
        <a asp-action="Details" asp-route-id="@user.UserId">See details</a>
    }
</ol>

<button class="btn btn-warning"><a asp-action="Create">Create User</a></button>